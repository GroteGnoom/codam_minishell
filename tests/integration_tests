#!/bin/bash
set -x
set -e

export ASAN_OPTIONS='detect_leaks=1'
export LSAN_OPTIONS='suppressions=leak_suppression.txt print_suppressions=0'

function check_valgrind_output()
{
	grep -q ' 0 errors from 0 contexts' leaks_output || { echo "found valgrind error for: \"$1\""; cat leaks_output; exit 1; }
}

function check_status()
{
	if [ $2 -ne $3 ]; then
		echo "found exit status difference for: \"$1\"";
		echo "bash exit status:" $3;
		echo "minishell exit status:" $2;
		exit 1;
	fi
}

function shell_test()
{
	> minishell_outfile
	> bash_outfile
	export OUTFILE=minishell_outfile
	set +e
	echo -e $1 | ../minishell >minishell_out 2>minishell_err
	export minishell_status=$?
	export OUTFILE=bash_outfile
	echo -e $1 | bash > bash_out 2> bash_err
	export bash_status=$?
	set -e
	diff minishell_out bash_out || { echo "std output test failed for: \"$1\""; exit 1; }
	diff minishell_err bash_err || { echo "error output test failed for: \"$1\""; exit 1; }
	diff minishell_outfile bash_outfile || { echo "outfile of output test failed for: \"$1\""; exit 1;}
	check_status "$1" $minishell_status $bash_status
}

#make DEBUG=1 LEAKS=1 -C ..
make DEBUG=1 -C ..

shell_test "echo 'hoi'"'"bla"'
#output_test 'cat < test_shell_split.c > $OUTFILE << hd -e | echo *$PATH '*'$PATH "*"$PATH *"$PATH" *'\''$PATH'\'' | cat -e\nbla\nhd\n'
#for the next two tests with multiple output redirections, to the same file, minishell always does the same thing, but bash doesn't. Bash seems to open the file from different forks and they put in information at the same time, which causes race conditions. Minishell puts in information in order.
#output_test 'rm $OUTFILE | cat >> $OUTFILE << hd1 << hd2 -e | >> $OUTFILE << hd3 | cat < test_shell_split.c << hd4 >> $OUTFILE | >> $OUTFILE echo *$PATH * $PATH "*"$PATH -e\n1\nhd1\n2\nhd2\n3\nhd3\n4\nhd4\n'
#output_test 'cat > $OUTFILE << hd1 << hd2 -e | >> $OUTFILE << hd3 | cat < test_shell_split.c >> $OUTFILE << hd4 -e\n1\nhd1\n2\nhd2\n3\nhd3\n4\nhd4\n' 
shell_test '<<'
shell_test '<< <'
shell_test '<< bla cat < test_shell_split.c\nasdf\nbla\n'
shell_test '< test_shell_split.c << bla cat\nasdf\nbla\n'
shell_test 'cat << bla\nasdf\nbla\n' 
shell_test '<< bla cat\nasdf\nbla\n' 
shell_test '<< bla\nasdf\nbla\n cat' 
shell_test 'cat << bla -e\nasdf\nbla\n' 
shell_test 'ls | cat << bla\nasdf\nbla\n' 
shell_test '<< bla cat\nasdf\nbla\necho banaan'
shell_test 'cat << bla | cat\nasdf\nbla\n' 
shell_test '<< bla cat | << bloe cat\nfirst\nbla\nsecond\nbloe\n' 
shell_test '<< bla cat | << bloe cat\nfirst\nbla\nsecond\nbloe\nls\n'
shell_test 'cat << bla | echo bla\nasdf\nbla\n' 
shell_test 'cat test_shell_split.c > $OUTFILE -e'
shell_test '< <'
shell_test '> <'
shell_test '|'
shell_test '< test_shell_split.c grep a | cat -e'
shell_test '< test_shell_split.c | cat -e > $OUTFILE'
shell_test '< test_shell_split.c | ls | cat -e > $OUTFILE'
shell_test '< test_shell_split.c grep a | > $OUTFILE'
shell_test '< test_shell_split.c grep a | cat -e > $OUTFILE'
shell_test '< test_shell_split.c grep "a"|grep "m" | wc -l|cat -e > $OUTFILE'
shell_test '<'
shell_test '>'
shell_test 'echo'
shell_test 'pwd > $OUTFILE'
shell_test '> $OUTFILE pwd'
shell_test 'ls < lol.c | grep "a"'
shell_test 'ls < test_shell_split.c | grep "a"'
shell_test 'cat > $OUTFILE < test_shell_split.c -e'
shell_test 'ls | cat > $OUTFILE < test_shell_split.c -e'
shell_test 'cat < test_shell_split.c > $OUTFILE -e'
shell_test 'cat > $OUTFILE < test_shell_split.c -e | ls'
shell_test 'ls | < integration_tests grep "a" | cat -e'
shell_test '> $OUTFILE ls -a'
shell_test '> > outfile'
shell_test 'ls | >'
shell_test 'ls | > >'
shell_test 'ls | > >>'
shell_test 'ls | <'
shell_test 'ls | banaan'
shell_test 'ls | "|"'
shell_test 'echo < |'
shell_test 'echo > |'
shell_test 'echo << |'
shell_test 'echo >> |'
shell_test 'ls|cat -e|cat -e'
shell_test 'ls|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e'
shell_test_no_valgrind 'ls|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e|cat -e